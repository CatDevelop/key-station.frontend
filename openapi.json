        "type": "object",
        "properties": {
          "customerId": {
            "type": "string"
          },
          "cellId": {
            "type": "string"
          }
        },
        "required": [
          "customerId",
          "cellId"
        ]
      },
      "ReturnKeyDto": {
        "type": "object",
        "properties": {
          "customerId": {
            "type": "string"
          },
          "cellId": {
            "type": "string"
          }
        },
        "required": [
          "customerId",
          "cellId"
        ]
      },
      "Cell": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "example": "4cd5975e-e958-45d6-b426-ad68b15bccd6"
          },
          "floor": {
            "type": "number",
            "example": 1
          },
          "classroom": {
            "type": "number",
            "example": 210
          },
          "isLocked": {
            "type": "boolean",
            "example": false
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time",
            "example": "2025-05-03T11:16:06.437Z"
          },
          "customerId": {
            "type": "string",
            "example": null
          }
        },
        "required": [
          "id",
          "floor",
          "classroom",
          "isLocked",
          "createdAt",
          "updatedAt",
          "customerId"
        ]
      },
      "FindAllLogResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "0efca53f-a44a-402c-a4fa-fb7011d8b1e9"
          },
          "action": {
            "type": "string",
            "enum": [
              "TAKEN",
              "RETURNED",
              "BLOCK",
              "UNBLOCK"
            ],
            "example": "BLOCK"
          },
          "cellId": {
            "type": "string",
            "format": "uuid",
            "example": "1d4e42ae-cd1a-4240-936e-144fbb99ec6d"
          },
          "customerId": {
            "type": "string",
            "format": "uuid",
            "example": "b6b84673-3699-464c-a41b-beabd8835d84"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "example": "2025-05-03T11:16:06.437Z"
          },
          "customer": {
            "$ref": "#/components/schemas/CreatedCustomerDto"
          },
          "cell": {
            "$ref": "#/components/schemas/Cell"
          }
        },
        "required": [
          "id",
          "action",
          "cellId",
          "customerId",
          "timestamp",
          "customer",
          "cell"
        ]
      },
      "FindAllLogsResponseDto": {
        "type": "object",
        "properties": {
          "logs": {
            "type": "array",
            "items": {
