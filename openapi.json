          "name": {
            "type": "string",
            "example": "Рожков Максим Евгеньевич"
          },
          "rfid": {
            "type": "string",
            "example": "123412"
          },
          "role": {
            "type": "string",
            "example": "TEACHER"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time",
            "example": "2025-05-03T11:16:06.437Z"
          }
