          "rfid": {
            "type": "string",
            "example": "123412"
          },
          "role": {
            "type": "string",
            "example": "TEACHER"
          }
        },
        "required": [
          "name",
          "rfid",
          "role"
        ]
      },
      "FindAllCustomersDto": {
        "type": "object",
        "properties": {
          "customers": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreatedCustomerDto"
            }
          }
        },
        "required": [
          "customers"
        ]
      },
      "FindByRfidCustomerResponseDto": {
        "type": "object",
        "properties": {
          "customer": {
            "$ref": "#/components/schemas/CreatedCustomerDto"
          }
        },
        "required": [
          "customer"
        ]
      },
      "UpdateCustomerDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Рожков Максим Евгеньевич"
          },
          "rfid": {
            "type": "string",
            "example": "123412"
          },
          "role": {
            "type": "string",
            "example": "TEACHER"
          },
          "id": {
            "type": "string"
          }
        },
        "required": [
